{
	"name": "@atlaskit/portal",
	"version": "5.1.0",
	"description": "A wrapper for rendering components in React portals.",
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
	},
	"repository": "https://bitbucket.org/atlassian/atlassian-frontend-mirror",
	"author": "Atlassian Pty Ltd",
	"license": "Apache-2.0",
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"module:es2019": "dist/es2019/index.js",
	"types": "dist/types/index.d.ts",
	"typesVersions": {
		">=4.5 <4.9": {
			"*": [
				"dist/types-ts4.5/*",
				"dist/types-ts4.5/index.d.ts"
			]
		}
	},
	"sideEffects": [
		"**/*.compiled.css"
	],
	"atlaskit:src": "src/index.tsx",
	"atlassian": {
		"team": "Design System Team",
		"website": {
			"name": "Portal",
			"category": "Libraries"
		},
		"runReact18": true
	},
	"af:exports": {
		"./types": "./src/entry-points/types.tsx",
		".": "./src/index.tsx"
	},
	"dependencies": {
		"@atlaskit/platform-feature-flags": "^1.1.0",
		"@atlaskit/theme": "^18.0.0",
		"@babel/runtime": "^7.0.0"
	},
	"peerDependencies": {
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"@af/accessibility-testing": "^2.0.0",
		"@af/integration-testing": "^0.5.0",
		"@atlaskit/button": "^21.1.0",
		"@atlaskit/code": "^16.0.0",
		"@atlaskit/docs": "^10.0.0",
		"@atlaskit/ds-lib": "^4.0.0",
		"@atlaskit/flag": "^16.2.0",
		"@atlaskit/icon": "^24.1.0",
		"@atlaskit/inline-dialog": "^17.0.0",
		"@atlaskit/link": "^3.0.0",
		"@atlaskit/modal-dialog": "^13.0.0",
		"@atlaskit/onboarding": "^13.0.0",
		"@atlaskit/primitives": "^14.1.0",
		"@atlaskit/section-message": "^8.1.0",
		"@atlaskit/ssr": "^0.4.0",
		"@atlaskit/tokens": "^4.3.0",
		"@atlaskit/tooltip": "^20.0.0",
		"@atlaskit/visual-regression": "^0.10.0",
		"@atlassian/feature-flags-test-utils": "^0.3.0",
		"@emotion/react": "^11.7.1",
		"@testing-library/react": "^13.4.0",
		"@testing-library/react-hooks": "^8.0.1",
		"bind-event-listener": "^3.0.0",
		"raf-stub": "^2.0.1",
		"typescript": "~5.4.2"
	},
	"keywords": [
		"atlaskit",
		"react",
		"ui"
	],
	"techstack": {
		"@atlassian/frontend": {
			"import-structure": "atlassian-conventions"
		},
		"@repo/internal": {
			"dom-events": "use-bind-event-listener",
			"design-system": "v1",
			"ui-components": "lite-mode",
			"analytics": "analytics-next",
			"design-tokens": [
				"color",
				"spacing"
			],
			"deprecation": "no-deprecated-imports",
			"styling": [
				"static",
				"emotion"
			]
		}
	},
	"homepage": "https://atlassian.design/components/portal/",
	"platform-feature-flags": {
		"platform_design_system_team_portal_logic_r18_fix": {
			"type": "boolean"
		}
	}
}
